{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/:domain",
      "destination": "/websites/:domain"
    },
    {
      "source": "/:domain/:path*",
      "destination": "/websites/:domain/:path*"
    }
  ]
}
